<h1>List of All Upcoming Interviews</h1>

<table class="table table-hover">
	<thead>
	  <tr>
		<th scope="col"></th>
		<th scope="col">Position</th>
		<th scope="col">Pay</th>
		<th scope="col">When</th>
	  </tr>
	</thead>
	<tbody class="table-group-divider" <% interviewDocs.forEach(function(i) { %> >
	  <tr>
		<th scope="row"></th>
		<td><%= i.position %></td>
		<td><%= i.pay %></td>
		<td><%= i.when %><a href="/interviews/<%= i._id %>">DETAILS</a></td>
		<% }); %>
		</tbody>
	  </tr>
	 
	  </tr>
	</tbody>
  </table>


