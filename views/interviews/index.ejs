<%- include('../partials/header') %>
<link rel="stylesheet" href="/stylesheets/style.css">
<table id="interviews">
    <thead>
      <tr>
        <th>List of All Interviews</th>
        <link rel="stylesheet" href="/stylesheets/style.css">
        <a href="/interviews/new">Add Upcoming Interviews</a>
        <th></th>
      </tr>
    </thead>
    <tbody>
      <% interviewDocs.forEach(function(i) { %>
        <tr>
          <td><%= i.position %></td>
          <td><%= i.pay %></td>
          <td><%= i.location %></td>
          <td><%= i.tips %></td>
          <td><a href="/interviews/<%= i._id %>">DETAILS</a></td>
        </tr>
        <% }); %>
      </tbody>
    
    </table>
    
